#include <iostream>
using namespace std;

int main() {

    //Declare first integer variable and initialize it
    int a = 8;

    //Declare second integer variable and intialize it
    int b = 10;

    //Declare a third variable and that adds the first two
    int sum = a + b;

    //Print the sum to the console
    cout << "The sum is: " << sum << endl;

    // Tels system program ended sucesfully 
    return 0;
}
