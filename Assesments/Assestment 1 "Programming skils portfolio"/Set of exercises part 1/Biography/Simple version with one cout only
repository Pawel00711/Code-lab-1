#include <iostream>
#include <string>
using namespace std;

int main() {
    string name = "Paul Watson";
    string hometown = "Birmingham";
    int age = 31;

// output in one cout
    cout << "Name: " << name << "\nHometown: " << hometown << "\nAge: " << age << endl;

// Tels system program ended sucesfully 
    return 0;
}
