#include <iostream>                        //Tells the compiler to include the Input/Output Stream library
#include <string>                          // Allow to use the string from C++ library
using namespace std;                       // Tells compiler to use namespace from std and allow to avoid use std:: everytime

//  Starting point of program and defines main function
int main() {
    cout << "coding is cool" << endl;     // Output statement
    return 0;                             // Tels system program ended sucesfully 
}
